{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build Hamada file",
            "command": "powershell",
            "args": [
                "-Command",
                "& C:\\msys64\\mingw64\\bin\\gcc.exe -fdiagnostics-color=always -g",
                "-I${workspaceFolder}",
                "-I${workspaceFolder}\\Service_Handlers\\ECUReset",
                "-I${workspaceFolder}\\Service_Handlers\\ReadWriteDID",
                "-I${workspaceFolder}\\Service_Handlers\\RequestDownload",
                "-I${workspaceFolder}\\Service_Handlers\\RequestTransferExit",
                "-I${workspaceFolder}\\Service_Handlers\\RoutineCTRL",
                "-I${workspaceFolder}\\Service_Handlers\\SecurityAccess",
                "-I${workspaceFolder}\\Service_Handlers\\SessionCTRL",
                "-I${workspaceFolder}\\Service_Handlers\\TesterPresent",
                "-I${workspaceFolder}\\Service_Handlers\\TransferData",
                "$(Get-ChildItem -Path ${workspaceFolder} -Filter *.c -Recurse | ForEach-Object { $_.FullName })",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
